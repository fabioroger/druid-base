{
  "name": "imply-base",
  "version": "3.1.0-1",
  "scripts": {
    "image-build": "docker build -t fabioroger/imply-base:$npm_package_version -t fabioroger/imply-base:latest .",
    "image-push": "docker push fabioroger/imply-base:$npm_package_version && docker push fabioroger/imply-base:latest",
    "docker": "npm run image-build && npm run image-push"
  }
}
